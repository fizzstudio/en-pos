{
  "name": "@fizz/en-pos",
  "version": "1.0.17",
  "description": "A better English POS tagger written in JavaScript",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.fizz.studio"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "dist/en-pos.esm.js",
  "scripts": {
    "tsc": "tsc",
    "build": "tsc && rollup -c",

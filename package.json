    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.7",
    "eslint": "^8.3.0",
    "jsdoc": "^3.6.7",
    "kis-jsdoc-plugin": "^1.4.9",
    "mocha": "^10.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "ts-node": "^2.1.0",
    "typescript": "^4.5.5"
  }
}

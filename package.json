    "@fizz/humannames": "^1.0.7"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fizzstudio/en-pos.git"
  },
  "keywords": [
    "pos",
    "tagger",
    "tagging",
    "part",
    "of",
    "speech",

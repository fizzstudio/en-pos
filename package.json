    "pubminor": "npm version minor && npm publish",
    "pubmajor": "npm version major && npm publish"
  },
  "author": "Alex Corvi <alex@arrayy.com>",
  "license": "MIT",
  "dependencies": {
    "@fizz/cities-list": "^1.0.4",
    "@fizz/en-inflectors": "^1.0.7",
